// tsconfig for use with Jest tests run via ts-jest
{
  "extends": "./tsconfig.json",
  "exclude": [".next", "node_modules"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "plugins": [
      {
        "transform": "ts-auto-mock/transformer",
        "cacheBetweenTests": false
      }
    ]
  },
}